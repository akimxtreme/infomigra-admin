<p id="notice"><%= notice %></p>

<h1>Country Categories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Percentage profile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @country_categories.each do |country_category| %>
      <tr>
        <td><%= country_category.name %></td>
        <td><%= country_category.description %></td>
        <td><%= country_category.percentage_profile %></td>
        <td><%= link_to 'Show', country_category %></td>
        <td><%= link_to 'Edit', edit_country_category_path(country_category) %></td>
        <td><%= link_to 'Destroy', country_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Country Category', new_country_category_path %>
