<p id="notice"><%= notice %></p>

<h1>Sexual Identities</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sexual_identities.each do |sexual_identity| %>
      <tr>
        <td><%= sexual_identity.name %></td>
        <td><%= sexual_identity.description %></td>
        <td><%= link_to 'Show', sexual_identity %></td>
        <td><%= link_to 'Edit', edit_sexual_identity_path(sexual_identity) %></td>
        <td><%= link_to 'Destroy', sexual_identity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sexual Identity', new_sexual_identity_path %>
