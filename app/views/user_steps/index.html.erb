<p id="notice"><%= notice %></p>

<h1>User Steps</h1>

<table>
  <thead>
    <tr>
      <th>Users</th>
      <th>Steps</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_steps.each do |user_step| %>
      <tr>
        <td><%= user_step.users %></td>
        <td><%= user_step.steps %></td>
        <td><%= link_to 'Show', user_step %></td>
        <td><%= link_to 'Edit', edit_user_step_path(user_step) %></td>
        <td><%= link_to 'Destroy', user_step, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Step', new_user_step_path %>
