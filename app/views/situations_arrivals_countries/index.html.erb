<p id="notice"><%= notice %></p>

<h1>Situations Arrivals Countries</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Percentage profile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @situations_arrivals_countries.each do |situations_arrivals_country| %>
      <tr>
        <td><%= situations_arrivals_country.name %></td>
        <td><%= situations_arrivals_country.description %></td>
        <td><%= situations_arrivals_country.percentage_profile %></td>
        <td><%= link_to 'Show', situations_arrivals_country %></td>
        <td><%= link_to 'Edit', edit_situations_arrivals_country_path(situations_arrivals_country) %></td>
        <td><%= link_to 'Destroy', situations_arrivals_country, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Situations Arrivals Country', new_situations_arrivals_country_path %>
